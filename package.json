{
  "name": "node-docker",
  "version": "1.0.0",
  "description": "learning node with docker",
  "main": "index.ts",
  "author": "savio777",
  "license": "MIT",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register ./index.ts",
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register ./index.ts",
    "prod": "tsc --noResolve --noCheck && tsc-alias && node ./dist/index.js"
  },
  "dependencies": {
    "@types/node": "^22.8.1",
    "express": "^4.21.1",
    "mongoose": "^8.7.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.7",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.6.3"
  }
}
